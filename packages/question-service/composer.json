{
    "name": "kids-qa-ai/question-service",
    "description": "Question and Answer service for Kids Q&A AI with WebSocket streaming",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kids Q&A AI Team",
            "email": "dev@kidsqaai.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0",
        "illuminate/contracts": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/queue": "^11.0",
        "pusher/pusher-php-server": "^7.0",
        "ratchet/pawl": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KidsQaAi\\QuestionService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KidsQaAi\\QuestionService\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KidsQaAi\\QuestionService\\QuestionServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
