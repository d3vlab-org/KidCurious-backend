{
    "name": "kids-qa-ai/media-service",
    "description": "Media service for YouTube Data API v3 with safeSearch=STRICT",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kids Q&A AI Team",
            "email": "dev@kidsqaai.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0",
        "illuminate/contracts": "^11.0",
        "illuminate/cache": "^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "google/apiclient": "^2.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KidsQaAi\\MediaService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KidsQaAi\\MediaService\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KidsQaAi\\MediaService\\MediaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
