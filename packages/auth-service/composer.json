{
    "name": "kids-qa-ai/auth-service",
    "description": "Supabase Auth wrapper service for Kids Q&A AI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kids Q&A AI Team",
            "email": "dev@kidsqaai.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0",
        "illuminate/contracts": "^11.0",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.0",
        "supabase/supabase-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KidsQaAi\\AuthService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KidsQaAi\\AuthService\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KidsQaAi\\AuthService\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
